container_commands:
  01_yarn_install:
    command: "yarn install"
  02_yarn_run_prod:
    command: "yarn run prod"
  03_php_artisan_config_cache:
    command: "sudo php artisan config:cache"
  04_php_artisan_migrate:
    command: "sudo php artisan migrate"
  05_php_artisan_optimize_clear:
    command: "sudo php artisan optimize:clear"
  06_php_artisan_optimize:
    command: "sudo php artisan optimize"